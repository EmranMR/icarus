{
  "actions" : {
    "build" : {
      "postActions" : [
        {
          "action" : "build",
          "name" : "Build LLDBAdapter",
          "taskName" : "LLDBAdapter",
          "type" : "runTask"
        },
        {
          "name" : "Build TestApplication",
          "script" : "swift build --product TestApplication",
          "type" : "runScript"
        }
      ]
    },
    "clean" : {
      "postActions" : [
        {
          "name" : "Clean",
          "script" : "swift package clean",
          "type" : "runScript"
        }
      ]
    }
  },
  "buildBeforeRunning" : true,
  "extension" : {
    "identifier" : "panic.Icarus",
    "name" : "Icarus"
  },
  "extensionTemplate" : "lldbDebug",
  "extensionValues" : {
    "launchPath" : ".build\/debug\/TestApplication",
    "runInRosetta" : false,
    "stopAtEntry" : false
  }
}
